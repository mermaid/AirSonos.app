{
  "name": "airsonos.app",
  "version": "0.1.0",
  "description": "A front end for AirSonos",
  "main": "index.js",
  "dependencies": {
    "airsonos": "^0.2.6",
    "electron-config": "^0.2.1",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "semver-compare": "^1.0.0"
  },
  "devDependencies": {
    "electron-installer-dmg": "^0.2.0",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.13"
  },
  "scripts": {
    "test": "test",
    "start": "electron .",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && electron-packager . AirSonos --out=dist --platform=darwin --arch=all --ignore=dist --ignore=README.md --ignore=.gitignore --icon=./img/airsonos-logo.icns",
    "postinstall": "npm rebuild --runtime=electron --target=1.4.13 --disturl=https://atom.io/download/atom-shell --build-from-sourcet"
  },
  "repository": {
    "type": "git",
    "url": "AirSonos.app"
  },
  "keywords": [
    "sonos",
    "airsonos",
    "airplay",
    "apple"
  ],
  "author": "Mermaid",
  "license": "MIT"
}
